{
  "name": "RingCentral Embeddable Engage Voice for HubSpot",
  "description": "Experimental Engage Voice browser extension for Hubspot",
  "version": "0.0.2",
  "permissions": [
    "http://*/",
    "https://*/",
    "storage",
    "activeTab",
    "tabs",
    "unlimitedStorage",
    "https://*.hubspot.com/*"
  ],
  "applications": {
    "gecko": {
      "id": "drake.zhao@ringcentral.com",
      "strict_min_version": "60.0"
    }
  },
  "browser_action": {
    "default_title": "Experimental Engage Voice browser extension for Hubspot",
    "default_icon": {
      "16": "icons/rc16.png",
      "32": "icons/rc32.png",
      "48": "icons/rc48.png",
      "128": "icons/rc128.png"
    }
  },
  "icons": {
    "16": "icons/rc16.png",
    "32": "icons/rc32.png",
    "48": "icons/rc48.png",
    "128": "icons/rc128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.hubspot.com/*"
      ],
      "js": [
        "./jsstore.worker.min.js",
        "./content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ringcentral.github.io/ringcentral-embeddable/index.html; object-src 'self'",
  "manifest_version": 2
}